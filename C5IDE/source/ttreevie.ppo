nItems  := val( alltrim( memoline( buffer,,nFrom)))

for n := 1 to nItems
    nFrom++
    ::Load( buffer, self, @nFrom )
next



return nil


   static FUNCTION TTreeView_Load( cInfo, oParent, nFrom ) ; local Self AS CLASS TTreeView := QSelf() AS CLASS TTreeView

