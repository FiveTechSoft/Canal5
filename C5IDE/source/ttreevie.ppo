   static FUNCTION TTreeView_LoadFile( cFileName ) ; local Self AS CLASS TTreeView := QSelf() AS CLASS TTreeView

local h
local nSize := fsize( cFileName )
local buffer
local nItems, n
local nFrom := 1

if nSize == 0
   MsgInfo( "No encuentro el fichero " + cFileName, "Atención" )
   return nil
endif
buffer := space( nSize )

h := fopen( cFileName, 0 )
fread( h, @buffer, nSize )
